<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.google.android.material.chip.Chip
        android:id="@+id/chip0"
        android:layout_width="422dp"
        android:layout_height="77dp"
        android:layout_marginStart="-100dp"
        android:layout_marginTop="250dp"
        android:layout_marginBottom="10dp"
        android:clickable="false"
        app:layout_constraintBottom_toTopOf="@+id/chip1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip1"
        android:layout_width="383dp"
        android:layout_height="77dp"
        android:layout_marginTop="19dp"
        android:layout_marginEnd="-100dp"
        android:clickable="false"
        app:layout_constraintBottom_toTopOf="@+id/button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chip0" />

    <EditText
        android:id="@+id/editTextPhone"
        android:layout_width="243dp"
        android:layout_height="62dp"
        android:layout_marginTop="310dp"
        android:layout_marginEnd="30dp"
        android:autoSizeTextType="uniform"
        android:autofillHints=""
        android:ems="10"
        android:fontFamily="@font/kurale"
        android:hint="@string/editPhoneMain"
        android:inputType="phone"
        android:textColorHint="#E65100"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="@+id/chip0"
        app:layout_constraintEnd_toEndOf="@+id/chip0"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.776" />

    <EditText
        android:id="@+id/editTextTextPassword"
        android:layout_width="237dp"
        android:layout_height="61dp"
        android:layout_marginEnd="7dp"
        android:autoSizeTextType="uniform"
        android:autofillHints=""
        android:ems="10"
        android:fontFamily="@font/kurale"
        android:hint="@string/editPasswordMain"
        android:inputType="textPassword"
        android:textColorHint="#E65100"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="@+id/chip1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.939"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextPhone"
        app:layout_constraintVertical_bias="0.843" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:autoSizeTextType="uniform"
        android:fontFamily="@font/kurale"
        android:text="@string/app_name"
        android:textAlignment="center"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.412"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="320dp"
        android:layout_height="287dp"
        android:layout_marginBottom="25dp"
        android:contentDescription="@string/ibookmainimage"
        app:layout_constraintBottom_toTopOf="@+id/chip0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/mainibook" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/oval_buttons"
        android:foreground="@drawable/ic_arrow_back_black_24dp"
        android:onClick="authorization"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/chip1"
        tools:ignore="OnClick,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="45dp"
        android:layout_height="40dp"
        android:layout_marginStart="-2dp"
        android:layout_marginTop="9dp"
        android:autoSizeTextType="uniform"
        android:text="@string/_7"
        android:textAlignment="textEnd"
        android:textColor="#E65100"
        app:layout_constraintBottom_toBottomOf="@+id/chip0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/editTextPhone"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>